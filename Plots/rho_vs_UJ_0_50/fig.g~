#gnuplot file
plot '../condensateData.txt' u 1 w l title ' ', \
     '../condensateData.txt' u 2 w l title ' ', \
     '../condensateData.txt' u 3 w l title ' ',\
     '../condensateData.txt' u 4 w l title ' ', \
     '../condensateData.txt' u 5 w l title ' ', \
     '../condensateData.txt' u 6 w l title ' ', \
     '../condensateData.txt' u 7 w l title ' '

set xlabel 'U/J'
set ylabel 'rho'
set terminal epslatex size 12cm, 7cm color colortext standalone
set output './fig.tex'
replot
